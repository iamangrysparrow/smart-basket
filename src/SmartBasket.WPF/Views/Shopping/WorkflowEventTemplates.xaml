<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SmartBasket.WPF.Views.Shopping"
                    xmlns:mdxaml="clr-namespace:MdXaml;assembly=MdXaml">

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- ÐžÐ±Ñ‰Ð¸Ðµ ÑÑ‚Ð¸Ð»Ð¸ -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <Style x:Key="EventTimestampStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="10"/>
        <Setter Property="Foreground" Value="{DynamicResource TextMutedBrush}"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <Style x:Key="SpinnerStyle" TargetType="Border">
        <Setter Property="Width" Value="14"/>
        <Setter Property="Height" Value="14"/>
        <Setter Property="CornerRadius" Value="7"/>
        <Setter Property="BorderThickness" Value="2"/>
        <Setter Property="BorderBrush" Value="{DynamicResource AccentBrush}"/>
        <Setter Property="Margin" Value="4,0"/>
        <Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
        <Setter Property="RenderTransform">
            <Setter.Value>
                <RotateTransform/>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <EventTrigger RoutedEvent="Loaded">
                <BeginStoryboard>
                    <Storyboard RepeatBehavior="Forever">
                        <DoubleAnimation Storyboard.TargetProperty="(Border.RenderTransform).(RotateTransform.Angle)"
                                         From="0" To="360" Duration="0:0:1"/>
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>
        </Style.Triggers>
    </Style>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- User Message Template -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <DataTemplate x:Key="UserMessageTemplate">
        <Grid Margin="60,4,12,4">
            <Border Background="{DynamicResource AccentBrush}"
                    CornerRadius="12,12,2,12"
                    Padding="12,8">
                <TextBlock Text="{Binding Text}"
                           TextWrapping="Wrap"
                           Foreground="White"/>
            </Border>
        </Grid>
    </DataTemplate>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- AI Message Template (with streaming support) -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <DataTemplate x:Key="AiMessageTemplate">
        <Grid Margin="12,4,60,4">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <!-- AI Avatar -->
            <Border Grid.Column="0"
                    Width="32" Height="32"
                    CornerRadius="16"
                    Background="{DynamicResource BackgroundLayer2Brush}"
                    Margin="0,0,8,0"
                    VerticalAlignment="Top">
                <TextBlock Text="ðŸ¤–"
                           FontSize="16"
                           HorizontalAlignment="Center"
                           VerticalAlignment="Center"/>
            </Border>

            <!-- Message -->
            <Border Grid.Column="1"
                    Background="{DynamicResource BackgroundLayer2Brush}"
                    CornerRadius="12,12,12,2"
                    Padding="12,8">
                <StackPanel>
                    <mdxaml:MarkdownScrollViewer Markdown="{Binding Text}"
                                                  MarkdownStyle="{DynamicResource ChatMarkdownStyle}"
                                                  Background="Transparent"
                                                  VerticalScrollBarVisibility="Disabled"/>

                    <!-- Typing indicator -->
                    <StackPanel Orientation="Horizontal"
                                Visibility="{Binding IsCompleted, Converter={StaticResource InverseBoolToVis}}">
                        <Border Style="{StaticResource SpinnerStyle}"/>
                        <TextBlock Text="Ð¿ÐµÑ‡Ð°Ñ‚Ð°ÐµÑ‚..."
                                   Style="{StaticResource EventTimestampStyle}"
                                   Margin="4,0,0,0"/>
                    </StackPanel>
                </StackPanel>
            </Border>
        </Grid>
    </DataTemplate>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- Tool Call Template (compact) -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <DataTemplate x:Key="ToolCallTemplate">
        <Border Margin="48,2,48,2"
                Background="{DynamicResource BackgroundLayer1Brush}"
                BorderBrush="{DynamicResource BorderBrush}"
                BorderThickness="1"
                CornerRadius="6"
                Padding="8,4">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="âš™ï¸" Margin="0,0,6,0"/>
                <TextBlock Text="{Binding ToolName}"
                           FontWeight="SemiBold"
                           Foreground="{DynamicResource AccentBrush}"/>
                <Border Style="{StaticResource SpinnerStyle}"
                        Visibility="{Binding IsCompleted, Converter={StaticResource InverseBoolToVis}}"/>
            </StackPanel>
        </Border>
    </DataTemplate>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- Tool Result Template (compact) -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <DataTemplate x:Key="ToolResultTemplate">
        <Border Margin="48,2,48,2"
                Background="{DynamicResource BackgroundLayer1Brush}"
                BorderBrush="{DynamicResource BorderBrush}"
                BorderThickness="1"
                CornerRadius="6"
                Padding="8,4">
            <StackPanel Orientation="Horizontal">
                <TextBlock Margin="0,0,6,0">
                    <TextBlock.Style>
                        <Style TargetType="TextBlock">
                            <Setter Property="Text" Value="âœ“"/>
                            <Setter Property="Foreground" Value="{DynamicResource SuccessBrush}"/>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ToolSuccess}" Value="False">
                                    <Setter Property="Text" Value="âœ—"/>
                                    <Setter Property="Foreground" Value="{DynamicResource ErrorBrush}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </TextBlock.Style>
                </TextBlock>
                <TextBlock Text="{Binding ToolName}"
                           Foreground="{DynamicResource TextMutedBrush}"/>
            </StackPanel>
        </Border>
    </DataTemplate>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- ÐšÐ°Ñ€Ñ‚Ð¾Ñ‡ÐºÐ° Ñ‚Ð¾Ð²Ð°Ñ€Ð° Ð²Ð½ÑƒÑ‚Ñ€Ð¸ Ð³Ñ€ÑƒÐ¿Ð¿Ñ‹ Ð¼Ð°Ð³Ð°Ð·Ð¸Ð½Ð° (Ð±ÐµÐ· Ð±ÐµÐ¹Ð´Ð¶Ð° Ð¼Ð°Ð³Ð°Ð·Ð¸Ð½Ð°) -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <DataTemplate x:Key="ProductCardCompletedTemplate">
        <Border Margin="0,0,0,2"
                Background="{DynamicResource BackgroundBaseBrush}"
                CornerRadius="4"
                Padding="8,6">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>

                <!-- Product emoji -->
                <Border Grid.Column="0"
                        Width="32" Height="32"
                        CornerRadius="6"
                        Background="{DynamicResource BackgroundLayer2Brush}"
                        Margin="0,0,8,0">
                    <TextBlock Text="{Binding ProductEmoji}"
                               FontSize="16"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"/>
                </Border>

                <!-- Product info -->
                <StackPanel Grid.Column="1" VerticalAlignment="Center">
                    <!-- Item name (Ñ‡Ñ‚Ð¾ Ð¸ÑÐºÐ°Ð»Ð¸) -->
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="âœ“"
                                   Foreground="{DynamicResource SuccessBrush}"
                                   FontSize="11"
                                   Margin="0,0,4,0"/>
                        <TextBlock Text="{Binding ProductName}"
                                   FontWeight="SemiBold"
                                   FontSize="12"/>
                    </StackPanel>
                    <!-- Selected product name -->
                    <TextBlock Text="{Binding SelectedProduct.Name}"
                               Foreground="{DynamicResource TextSecondaryBrush}"
                               FontSize="11"
                               TextTrimming="CharacterEllipsis"
                               MaxWidth="280"/>
                    <!-- Size Ã— Quantity -->
                    <TextBlock Text="{Binding QuantityText}"
                               Foreground="{DynamicResource TextMutedBrush}"
                               FontSize="10"/>
                </StackPanel>

                <!-- Price (LineTotal) -->
                <TextBlock Grid.Column="2"
                           VerticalAlignment="Center"
                           FontSize="14"
                           FontWeight="Bold"
                           Foreground="{DynamicResource AccentBrush}">
                    <Run Text="{Binding LineTotal, StringFormat='{}{0:N2}', Mode=OneWay}"/>
                    <Run Text="â‚½"/>
                </TextBlock>
            </Grid>
        </Border>
    </DataTemplate>

    <!-- ÐšÐ°Ñ€Ñ‚Ð¾Ñ‡ÐºÐ° Ð¾ÑˆÐ¸Ð±ÐºÐ¸ Ð¿Ð¾Ð¸ÑÐºÐ° Ð²Ð½ÑƒÑ‚Ñ€Ð¸ Ð³Ñ€ÑƒÐ¿Ð¿Ñ‹ Ð¼Ð°Ð³Ð°Ð·Ð¸Ð½Ð° -->
    <DataTemplate x:Key="ProductCardFailedTemplate">
        <Border Margin="0,0,0,2"
                Background="{DynamicResource ErrorBackgroundBrush}"
                CornerRadius="4"
                Padding="8,6">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="âœ—"
                           Foreground="{DynamicResource ErrorBrush}"
                           FontSize="11"
                           Margin="0,0,4,0"/>
                <TextBlock Text="{Binding ProductName}"
                           FontWeight="SemiBold"
                           FontSize="12"/>
                <TextBlock Text=": "
                           Foreground="{DynamicResource TextMutedBrush}"
                           FontSize="12"/>
                <TextBlock Text="{Binding Text}"
                           TextWrapping="NoWrap"
                           TextTrimming="CharacterEllipsis"
                           Foreground="{DynamicResource TextSecondaryBrush}"
                           FontSize="12"
                           MaxWidth="300"/>
            </StackPanel>
        </Border>
    </DataTemplate>

    <!-- ProductCardTemplateSelector Ð´Ð»Ñ Ð¸ÑÐ¿Ð¾Ð»ÑŒÐ·Ð¾Ð²Ð°Ð½Ð¸Ñ Ð² StoreGroupTemplate -->
    <local:ProductCardTemplateSelector x:Key="ProductCardTemplateSelector"
                                        ProductSelectionCompletedTemplate="{StaticResource ProductCardCompletedTemplate}"
                                        ProductSelectionFailedTemplate="{StaticResource ProductCardFailedTemplate}"/>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- Store Group Template â€” Ð³Ñ€ÑƒÐ¿Ð¿Ð° ÑÐ¾Ð±Ñ‹Ñ‚Ð¸Ð¹ Ð¿Ð¾ Ð¼Ð°Ð³Ð°Ð·Ð¸Ð½Ñƒ -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <DataTemplate x:Key="StoreGroupTemplate">
        <Border Margin="12,4,12,4"
                Background="{DynamicResource BackgroundLayer1Brush}"
                BorderBrush="{DynamicResource BorderBrush}"
                BorderThickness="1"
                CornerRadius="8"
                Padding="12">
            <StackPanel>
                <!-- Header: Store badge + Progress -->
                <Grid Margin="0,0,0,8">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>

                    <!-- Store badge -->
                    <Border Grid.Column="0"
                            CornerRadius="4"
                            Padding="8,4"
                            Margin="0,0,12,0"
                            VerticalAlignment="Center">
                        <Border.Background>
                            <SolidColorBrush Color="{Binding StoreColor}"/>
                        </Border.Background>
                        <TextBlock Text="{Binding StoreName}"
                                   Foreground="White"
                                   FontSize="12"
                                   FontWeight="SemiBold"/>
                    </Border>

                    <!-- Progress bar + text -->
                    <StackPanel Grid.Column="1" VerticalAlignment="Center">
                        <DockPanel Margin="0,0,0,4">
                            <TextBlock DockPanel.Dock="Right"
                                       Foreground="{DynamicResource TextMutedBrush}"
                                       FontSize="12">
                                <Run Text="{Binding CompletedCount, Mode=OneWay}"/>
                                <Run Text=" Ð¸Ð· "/>
                                <Run Text="{Binding TotalCount, Mode=OneWay}"/>
                            </TextBlock>
                            <TextBlock Text="ÐŸÐ¾Ð¸ÑÐº Ñ‚Ð¾Ð²Ð°Ñ€Ð¾Ð²"
                                       Foreground="{DynamicResource TextSecondaryBrush}"
                                       FontSize="12"/>
                        </DockPanel>
                        <ProgressBar Minimum="0"
                                     Maximum="100"
                                     Value="{Binding ProgressPercent, Mode=OneWay}"
                                     Height="12"
                                     Background="{DynamicResource BackgroundLayer2Brush}"
                                     Foreground="{DynamicResource AccentBrush}"/>
                    </StackPanel>

                    <!-- Spinner or check -->
                    <Grid Grid.Column="2" Margin="12,0,0,0" VerticalAlignment="Center">
                        <Border Style="{StaticResource SpinnerStyle}"
                                Visibility="{Binding IsSearchCompleted, Converter={StaticResource InverseBoolToVis}}"/>
                        <TextBlock Text="âœ“"
                                   FontSize="16"
                                   Foreground="{DynamicResource SuccessBrush}"
                                   Visibility="{Binding IsSearchCompleted, Converter={StaticResource BoolToVis}}"/>
                    </Grid>
                </Grid>

                <!-- Items list -->
                <ItemsControl ItemsSource="{Binding Items}"
                              ItemTemplateSelector="{StaticResource ProductCardTemplateSelector}"/>
            </StackPanel>
        </Border>
    </DataTemplate>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- Ð¡Ñ‚Ð°Ñ€Ñ‹Ðµ ÑˆÐ°Ð±Ð»Ð¾Ð½Ñ‹ Ð´Ð»Ñ Ð¾Ð±Ñ€Ð°Ñ‚Ð½Ð¾Ð¹ ÑÐ¾Ð²Ð¼ÐµÑÑ‚Ð¸Ð¼Ð¾ÑÑ‚Ð¸ (ÐµÑÐ»Ð¸ Ð¸ÑÐ¿Ð¾Ð»ÑŒÐ·ÑƒÑŽÑ‚ÑÑ Ð½Ð°Ð¿Ñ€ÑÐ¼ÑƒÑŽ) -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <DataTemplate x:Key="ProductSelectionCompletedTemplate">
        <ContentControl Content="{Binding}"
                        ContentTemplate="{StaticResource ProductCardCompletedTemplate}"/>
    </DataTemplate>

    <DataTemplate x:Key="ProductSelectionFailedTemplate">
        <ContentControl Content="{Binding}"
                        ContentTemplate="{StaticResource ProductCardFailedTemplate}"/>
    </DataTemplate>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- System Message Template -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <DataTemplate x:Key="SystemMessageTemplate">
        <Border Margin="24,4,24,4"
                Background="{DynamicResource WarningBackgroundBrush}"
                CornerRadius="6"
                Padding="10">
            <TextBlock Text="{Binding Text}"
                       TextWrapping="Wrap"
                       Foreground="{DynamicResource WarningBrush}"
                       FontStyle="Italic"/>
        </Border>
    </DataTemplate>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- Error Template -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <DataTemplate x:Key="ErrorTemplate">
        <Border Margin="24,4,24,4"
                Background="{DynamicResource ErrorBackgroundBrush}"
                BorderBrush="{DynamicResource ErrorBrush}"
                BorderThickness="1"
                CornerRadius="6"
                Padding="10">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="âš ï¸ " FontSize="14"/>
                <TextBlock Text="{Binding Text}"
                           TextWrapping="Wrap"
                           Foreground="{DynamicResource ErrorBrush}"/>
            </StackPanel>
        </Border>
    </DataTemplate>

    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->
    <!-- Template Selector (used in ItemsControl) -->
    <!-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• -->

    <local:WorkflowEventTemplateSelector x:Key="WorkflowEventTemplateSelector"
                                          UserMessageTemplate="{StaticResource UserMessageTemplate}"
                                          AiMessageTemplate="{StaticResource AiMessageTemplate}"
                                          ToolCallTemplate="{StaticResource ToolCallTemplate}"
                                          ToolResultTemplate="{StaticResource ToolResultTemplate}"
                                          StoreGroupTemplate="{StaticResource StoreGroupTemplate}"
                                          SystemMessageTemplate="{StaticResource SystemMessageTemplate}"
                                          ErrorTemplate="{StaticResource ErrorTemplate}"/>

</ResourceDictionary>
